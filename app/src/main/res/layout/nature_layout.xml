<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="10dp"
    android:gravity="center"
    tools:context=".natureClass">

    <ImageView
        android:id="@+id/wallpaper1"
        android:layout_width="186dp"
        android:layout_height="184dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="24dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/jungles" />

    <ImageView
        android:id="@+id/wallpaper2"
        android:layout_width="186dp"
        android:layout_height="184dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="40dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/wallpaper1"
        app:srcCompat="@drawable/mountains" />

    <ImageView
        android:id="@+id/wallpaper3"
        android:layout_width="186dp"
        android:layout_height="184dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/wallpaper5"
        app:srcCompat="@drawable/sands" />

    <ImageView
        android:id="@+id/wallpaper4"
        android:layout_width="186dp"
        android:layout_height="184dp"
        android:layout_marginEnd="100dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/wallpaper3"
        app:layout_constraintVertical_bias="0.302"
        app:srcCompat="@drawable/waterfalls" />

    <ImageView
        android:id="@+id/wallpaper5"
        android:layout_width="186dp"
        android:layout_height="184dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/moon" />

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="276dp"
        android:text="Back"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/wallpaper4" />
</androidx.constraintlayout.widget.ConstraintLayout>